    
        <div id="{{this.id}}" class="panel-body">{{this.bookName}}
        {{#if this.completed}}
            <span class="pull-right">Completed!</span>
        {{else}}
        <span class="pull-right">
            <i class="fa fa-check accept" aria-hidden="true"></i>
            <i class="fa fa-times cancel" aria-hidden="true"></i>


        </span>
        {{/if}}
        </div>
 
